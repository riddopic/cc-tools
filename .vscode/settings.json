{
  "files.exclude": {
    "**/.ruff_cache": true,
    "**/.benchmarks": true,
    "**/.tox": true,
    "**/.venv": true,
    "**/.pytest_cache": true,
    "**/**/coverage.*": true,
    "**/**/coverage.html": true,
    "**/**/coverage_gcp.out": true,
    "**/**/coverage-test-output.*": true,
    "**/**/cover.out": true,
    "**/**/integration-test-output.*": true,
    "**/**/test-output.*": true,
    "**/**/unit-test-output.*": true,
    "bin/**": true,
    ".mockcache": true,
    ".mockgen-cache": true,
    ".task": true,
    ".windsurf": true,
    "vendor": true,
    "coverage": true,
    "quanta": true,
    "go.sum": true,
    "out": true,
    "cache": true,
    ".serena": true,
    "foundry.lock": true,
    ".gocache": true,
  },
  // put a ruler on the editing screen at 120 characters
  "editor.rulers": [
    120
  ],
  "gopls": {
    "buildFlags": [
      "-tags=test"
    ],
    "ui.semanticTokens": true,
    "ui.completion.usePlaceholders": true
  },
  "go.lintTool": "golangci-lint",
  "go.lintOnSave": "workspace",
  "go.lintFlags": [
    "--allow-parallel-runners"
  ],
  "go.toolsEnvVars": {
    "GOPATH": "/Users/riddopic/go"
  },
  "go.alternateTools": {
    "customFormatter": "golangci-lint",
  },
  "go.enableCodeLens": {
    "runtest": true
  },
  "go.formatTool": "goimports",
  "go.formatFlags": [
    "-local",
    "github.com/riddopic/quanta"
  ],
  "editor.formatOnSave": true,
  "files.trimTrailingWhitespace": true,
  "[go]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "[solidity]": {
    "editor.defaultFormatter": "JuanBlanco.solidity"
  },
  "solidity.formatter": "forge",
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "cSpell.ignorePaths": [
    "go.sum",
    "go.mod",
    "vendor/**",
    ".git",
    ".gitignore",
    "**/dist/**",
    ".vscode/**",
    ".golangci.yml",
    "Makefile",
    ".mockery.yml",
